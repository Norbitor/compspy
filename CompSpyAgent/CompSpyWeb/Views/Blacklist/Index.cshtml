@model IEnumerable<CompSpyWeb.Models.Blacklist>
@{
    ViewBag.Title = "Blacklista";
}

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>Nazwa procesu</th>
        <th>Sala</th>
        <th>Operacje</th>
    </tr>

    @foreach (var blacklist in Model)
    {
        <tr>
            <td>@blacklist.ProcessName</td>
            <td>@blacklist.Classroom.Name</td>
            <td>
                @Html.ActionLink("Edytuj", "Edit", "Blacklist", new { id = @blacklist.BlacklistID }, new { @class = "btn btn-primary" })
                <a href="#" data-toggle="modal" data-target="#delConfirmDialog" class="btn btn-danger delopt" data-id="@blacklist.BlacklistID">Usuń</a>
            </td>
        </tr>
    }
</table>

<p>
    @Html.ActionLink("Dodaj element", "New", null, new { @class = "btn btn-primary" })
</p>


